.pyg_index .index_floorImg {
  padding: 20rpx 15rpx;
}

.pyg_index .index_floorImg navigator {
  width: 232rpx;
  height: 386rpx;
}

.pyg_index .index_floorImg navigator:nth-last-child(-n + 4) {
  height: 188rpx;
  margin-left: 11rpx;
}

.pyg_index .index_floorImg navigator:nth-last-child(-n + 2) {
  margin-top: 10rpx;
}

.pyg_index .index_floorImg navigator image {
  width: 100%;
  height: 100%;
}

.pyg_index .index_floorImg .typeOne:nth-last-child(-n + 4) {
  width: 233rpx;
}

.pyg_index .index_floorImg .typeTwo:nth-child(3), .pyg_index .index_floorImg .typeTwo:nth-child(4) {
  width: 193rpx;
}

.pyg_index .index_floorImg .typeTwo:nth-child(2), .pyg_index .index_floorImg .typeTwo:nth-child(5) {
  width: 273rpx;
}
